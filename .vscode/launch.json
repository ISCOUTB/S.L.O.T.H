{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: SQL Builder Server",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/apps/backend/parsers/sql-builder",
            "program": "./src/server.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Excel Reader Server",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/apps/backend/parsers/excel-reader",
            "program": "./src/server_rest.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: DDLGenerator Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/apps/backend/parsers/ddl-generator/src/server.py",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/apps/backend/parsers/formula-parser/node_modules/@grpc/grpc-js/build/src/server-interceptors.js"
        },
        {
            "name": "Python Debugger: Database Connection Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/apps/connections/database/src/server.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Messaging Connection Server",
            "type": "debugpy",
            "request": "launch",
            "module": "src.server",
            "cwd": "${workspaceFolder}/apps/connections/messaging"
        },
        {
            "name": "Python Debugger: Typechecking",
            "type": "debugpy",
            "request": "launch",
            "module": "src.main",
            "cwd": "${workspaceFolder}/apps/backend/typechecking"
        },
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "src.main:app",
                "--reload"
            ],
            "jinja": true,
            "cwd": "${workspaceFolder}/apps/backend/api"
        }
    ]
}