{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: SQL Builder Server",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/excel-parsing/sql-builder",
            "program": "./src/server.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Excel Reader",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/excel-parsing/excel-reader",
            "program": "./src/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: DDLGenerator Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/excel-parsing/ddl-generator/src/server.py",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/excel-parsing/formula-parser/node_modules/@grpc/grpc-js/build/src/server-interceptors.js"
        },
        {
            "name": "Python Debugger: Workers",
            "type": "debugpy",
            "request": "launch",
            "module": "app.workers.worker_manager",
            "cwd": "${workspaceFolder}/typechecking/backend"
        },
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload"
            ],
            "jinja": true,
            "cwd": "${workspaceFolder}/typechecking/backend"
        }
    ]
}